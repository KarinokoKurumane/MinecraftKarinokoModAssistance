<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="LoaderSettingsBehaviour">
      <HintPath>..\..\LoaderSettingsBehaviour\LoaderSettingsBehaviour\bin\Debug\net8.0-windows\LoaderSettingsBehaviour.dll</HintPath>
      <EmbedInteropTypes>False</EmbedInteropTypes>
      <Private>True</Private>
    </Reference>
    <Reference Include="UpdateBehaviour">
      <HintPath>..\..\UpdateBehaviour\UpdateBehaviour\bin\Debug\net8.0-windows\UpdateBehaviour.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
