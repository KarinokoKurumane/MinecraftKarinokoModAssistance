<Window x:Class="MinecraftKarinokoModAssistance.LauncherType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MinecraftKarinokoModAssistance"
        mc:Ignorable="d"
        Title="Wybierz Loader Modów" Height="200" Width="400" Background="#FF1E1E1E" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Wybierz platformę modowania:" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,20,0,0" Foreground="White"/>
        <ComboBox x:Name="Cbx_LoaderList" HorizontalAlignment="Center" VerticalAlignment="Center" Width="200"/>
        <Button Content="Dalej" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,20" Width="100" Click="Btn_Confirm_Click"/>
    </Grid>


</Window>
